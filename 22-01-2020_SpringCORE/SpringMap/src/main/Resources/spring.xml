<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.1.xsd">
           
           <bean id = "emp1" class = "SpringAssignment.Employee.Employee">
           <property name = "name" value = "Saurabh Singh"></property>
           <property name="id" value = "1"></property>
           <property name="company" value = "Accolite"></property>
           </bean>
           
           <bean id = "emp2" class = "SpringAssignment.Employee.Employee">
           <property name = "name" value = "Umed Rajawat"></property>
           <property name="id" value = "2"></property>
           <property name="company" value = "KickDrum"></property>
           </bean>
           
           <bean id = "emp3" class = "SpringAssignment.Employee.Employee">
           <property name = "name" value = "Yash Kesharwani"></property>
           <property name="id" value = "3"></property>
           <property name="company" value = "Hp"></property>
           
           </bean>
           
        
           
           <bean id = "empMap" class = "SpringAssignment.Employee.EmployeeMap">
           <property name = "empMap">
           <map>
           <entry key = "1" value-ref = "emp1"></entry>
           <entry key = "2" value-ref = "emp2"></entry>
           <entry key = "3" value-ref = "emp3"></entry>
           </map>
           </property>
           </bean>          
           
</beans>